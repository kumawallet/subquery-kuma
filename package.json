{
  "name": "subquery-nova",
  "version": "1.0.0",
  "description": "Nova SubQuery projects",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/subql build",
    "start:docker:evm": "docker-compose -f docker-compose.evm.yml pull && docker-compose -f docker-compose.evm.yml up --remove-orphans",
    "start:docker:substrate": "docker-compose -f docker-compose.substrate.yml pull && docker-compose -f docker-compose.substrate.yml up --remove-orphans",
    "codegen": "./node_modules/.bin/subql codegen",
    "validate": "./node_modules/.bin/subql validate"
  },
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "Jay Ji",
  "license": "Apache-2.0",
  "devDependencies": {
    "@polkadot/api": "^10.11.2",
    "@polkadot/api-derive": "^10.11.2",
    "@polkadot/types": "^10.11.2",
    "@polkadot/types-augment": "^8",
    "@polkadot/types-codec": "^10.11.2",
    "@subql/cli": "latest",
    "@subql/node-ethereum": "latest",
    "@subql/testing": "latest",
    "@subql/types": "latest",
    "@subql/types-ethereum": "^3.4.0",
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "moonbeam-types-bundle": "^2.0.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "ipfs-unixfs": "6.0.6"
  }
}
